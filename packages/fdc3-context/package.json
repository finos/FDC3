    "eslint": "^9.32.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "28.8.3",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "globals": "^15.14.0",
    "mkdirp": "^3.0.1",
    "quicktype": "23.0.78",
    "rimraf": "^6.0.1",
    "ts-jest": "29.2.5",
    "tslib": "^2.7.0",
    "typescript": "^5.6.3"
  }
