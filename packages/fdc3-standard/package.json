    "@eslint/js": "^9.19.0",
    "@types/jest": "29.5.13",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "28.8.3",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "globals": "^15.14.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-junit": "^16.0.0",
