    "url": "git+https://github.com/finos/FDC3.git"
  },
  "publishConfig": {
    "tag": "latest"
  },
  "license": "Apache-2.0",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
